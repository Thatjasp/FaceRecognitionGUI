<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GUIController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="end_btn" contentDisplay="CENTER" minWidth="24.0" mnemonicParsing="false" onAction="#endCamera" prefHeight="26.0" prefWidth="71.0" text="End">
               <padding>
                  <Insets right="20.0" />
               </padding>
            </Button>
            <Button id="start_btn" fx:id="start_btn" alignment="CENTER" contentDisplay="CENTER" disable="true" mnemonicParsing="false" onAction="#startCamera" prefHeight="26.0" prefWidth="187.0" text="Start Camera" textAlignment="CENTER" />
        </buttons>
      </ButtonBar>
   </bottom>
   <top>
      <HBox alignment="CENTER">
         <children>
            <ButtonBar>
               <buttons>
                  <ToolBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <items>
                        <CheckBox fx:id="logoBox" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#loadLogo" text="Logo Box" />
                        <CheckBox fx:id="greyScale" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#greyBox" text="Grey Scale" />
                        <CheckBox fx:id="EyeTrack" mnemonicParsing="false" onAction="#eyeReckingBox" text="Face Recognition" />
                    </items>
                  </ToolBar>
               </buttons>
            </ButtonBar>
         </children>
      </HBox>
   </top>
   <center>
      <Pane prefHeight="190.0" prefWidth="295.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="currentFrame" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </Pane>
   </center>
</BorderPane>
